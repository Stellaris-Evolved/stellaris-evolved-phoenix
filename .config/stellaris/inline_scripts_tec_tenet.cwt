parameters = {
    ## context_key = inline_script@cui/tenet/tec_tenet_declarations
    ## context_configs_type = multiple
    extra = {
        ## cardinality = 1..1
        _step = enum[tec_tenet_include_step]
        ## cardinality = 0..1
        _tenet = enum[tec_tenet]
        ## cardinality = 0..1
        _pass = { }
        ## cardinality = 0..1
        ## context_configs_type = multiple
        ## inherit
        _code = { }
    }

    ## context_key = inline_script@cui/tenet/tec_tenet_option_value[anything]
    name = enum[tec_tenet]
    ## context_key = inline_script@cui/tenet/tec_tenet_option_value[anything]
    ## replace_scopes = { this = country  root = any }
    ## context_configs_type = multiple
    potential = single_alias_right[trigger_clause]
    ## context_key = inline_script@cui/tenet/tec_tenet_option_value[anything]
    ## replace_scopes = { this = country  root = any }
    ## context_configs_type = multiple
    allow = single_alias_right[trigger_clause]
    ## context_key = inline_script@cui/tenet/tec_tenet_option_value[anything]
    ## replace_scopes = { this = country  root = any }
    ## context_configs_type = multiple
    on_activate = single_alias_right[effect_clause]
    ## context_key = inline_script@cui/tenet/tec_tenet_option_value[anything]
    ## replace_scopes = { this = country  root = any }
    ## context_configs_type = multiple
    on_deactivate = single_alias_right[effect_clause]
    ## context_key = inline_script@cui/tenet/tec_tenet_option_value[anything]
    ## replace_scopes = { this = country  root = any }
    ## context_configs_type = multiple
    modifier = single_alias_right[modifier_clause]
    ## context_key = inline_script@cui/tenet/tec_tenet_option_value[anything]
    bool = hide_modifier
    ## context_key = inline_script@cui/tenet/tec_tenet_option_value[anything]
    bool = no_modifier
    ## context_key = inline_script@cui/tenet/tec_tenet_option_value[anything]
    ## file_extensions = { .dds .png .tga }
    icon = filepath
    ## context_key = inline_script@cui/tenet/tec_tenet_option_value[anything]
    category = enum[tec_tenet_category]
    ## context_key = inline_script@cui/tenet/tec_tenet_option_value[anything]
    ## replace_scopes = { this = country  root = any }
    ## context_configs_type = multiple
    cost = single_alias_right[modifier_rule_clause]
    ## context_key = inline_script@cui/tenet/tec_tenet_option_value[anything]
    ## replace_scopes = { this = country  root = any }
    ## context_configs_type = multiple
    ai_weight = single_alias_right[modifier_rule_clause]

    ## context_key = script_value@tec_tenet_cost
    TENET = enum[tec_tenet]
    ## context_key = script_value@tec_tenet_ai_weight
    TENET = enum[tec_tenet]
}


